  "scripts": {
    "build-storybook": "build-storybook",
    "build:client": "webpack --config ./config/webpack.config.prod.js --progress --profile --colors",
    "build:server": "cross-env webpack --config ./config/webpack.config.server.js --progress --colors",
    "dev": "PWA_ENV=qa NODE_ENV=development webpack-dev-server --open --config ./config/webpack.config.dev.js",
    "dev:app": "cross-env PWA_ENV=development NODE_ENV=development webpack --config ./config/webpack.config.prod.js --progress --profile --colors --watch && PWA_ENV=development NODE_ENV=development yarn run build:server --watch && yarn run dev:start",
    "dev:build": "cross-env PWA_ENV=development NODE_ENV=development run-s build:*",
    "dev:server": "NODE_ENV=development node server.prod.js",
    "dev:start": "PWA_ENV=development NODE_ENV=development nodemon ./build/server/main.js",
    "dev:watch": "PWA_ENV=development NODE_ENV=development yarn run build:client --watch",
    "eslint-fix": "eslint ./src/* --fix",
    "production:build": "PUBLIC_PATH=//ui.cltpstatic.com/ ENABLE_SW=true PWA_ENV=production NODE_ENV=production run-s build:*",
    "qa:app": "cross-env PWA_ENV=qa NODE_ENV=production webpack --config ./config/webpack.config.prod.js --progress --profile --colors --watch && PWA_ENV=qa NODE_ENV=production yarn run build:server --watch && yarn run qa:start",
    "qa:build": "PUBLIC_PATH=//qa2.cltpstatic.com/ ENABLE_SW=true PWA_ENV=qa NODE_ENV=production run-s build:*",
    "qa:start": "PWA_ENV=qa NODE_ENV=production node ./build/server/main.js",
    "server": "node server.js",
    "server:prod": "NODE_ENV=production node server.prod.js",
    "start": "concurrently --kill-others \"yarn run dev\" \"yarn run server\"",
    "storybook": "start-storybook -p 6006"
  },
  "version": "1.0.0"
